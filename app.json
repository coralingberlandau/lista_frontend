{
  "expo": {
    "name": "lista",
    "slug": "lista",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lista.png",
    "scheme": "http://localhost:8081/",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/lista.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lista.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/lista.png",
      "linking": {
        "enabled": true,
        "prefixes": ["https://lista-project.netlify.app/"],
        "config": {
          "screens": {
            "ChangePassword": "change-password"
          }
        }
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
